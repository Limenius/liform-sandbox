{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "no-redeclare": 0,
    "no-console": 2,
  },
  "plugins": [
    "react"
  ]
}
